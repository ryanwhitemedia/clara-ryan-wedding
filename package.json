{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/ryanwhitemedia/nextjs-starter"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanwhitemedia/nextjs-starter/issues"
  },
  "scripts": {
    "dev": "cross-env BUILD_ENV=development next dev",
    "start": "next start",
    "\n ######################  NPM Secondary Pipeline  ######################": "",
    "build": "next build",
    "build:ssr": "next build",
    "build:static": "next build && next export",
    "postbuild:static": "npm-run-all postcss next-sitemap",
    "ci-prod-server": "serve out &",
    "next-sitemap": "next-sitemap",
    "postcss": "postcss ./out/**/*.css --use css-declaration-sorter --replace --no-map",
    "robots-txt-generator": "node scripts/robots-txt-generator.js",
    "\n ######################  Support Development Tools  ######################": "",
    "generate": "sg wizard",
    "\n ######################  Automation Tools  ######################": "",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "linters": "npm-run-all -p lint sass-lint ls-lint",
    "js-lint": "eslint \"./src/**/*.js\" -c ./.eslintrc.json --quiet --ignore-pattern .gitignore",
    "sass-lint": "stylelint \"./src/**/*.scss\"",
    "ls-lint": "node_modules/.bin/ls-lint"
  },
  "dependencies": {
    "@jam3/detect": "2.0.5",
    "@jam3/react-hooks": "1.0.0",
    "@reduxjs/toolkit": "^1.6.0",
    "classnames": "2.3.1",
    "default-passive-events": "2.0.0",
    "focus-visible": "5.2.0",
    "get-scroll": "2.0.1",
    "gsap": "3.8.0",
    "js-cookie": "^3.0.0",
    "next": "^12.0.9",
    "next-pwa": "^5.4.0",
    "no-op": "1.0.3",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.1",
    "redux": "^4.1.2",
    "swr": "^1.1.0",
    "xss": "^1.0.10",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jam3/react-check-extra-props": "^1.0.1",
    "@jam3/stats": "1.0.1",
    "@ls-lint/ls-lint": "^1.10.0",
    "@next/bundle-analyzer": "12.0.7",
    "@svgr/webpack": "^5.5.0",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.1.3",
    "eslint": "7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-postcss": "4.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-config-react-jam3": "1.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jam3": "0.2.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "generate-robotstxt": "^8.0.3",
    "husky": "^4.2.5",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-pngquant": "9.0.2",
    "lint-staged": "^12.1.2",
    "maxstache": "^1.0.7",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "next-compose-plugins": "2.2.1",
    "next-optimized-images": "2.6.2",
    "next-sitemap": "^1.6.203",
    "npm-run-all": "4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2",
    "react-dev-utils": "11.0.4",
    "redux-devtools-extension": "2.13.9",
    "responsive-loader": "^2.3.0",
    "sass": "1.43.4",
    "sass-loader": "12.3.0",
    "seng-generator": "^0.9.0",
    "serve": "13.0.2",
    "shell-quote": "^1.7.3",
    "stylelint": "13.13.1",
    "stylelint-performance-animation": "^1.2.2",
    "webp-loader": "0.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=14.17.6",
    "npm": ">=7.24.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"Pre-commit checks...\" && lint-staged",
      "pre-push": "echo \"Pre-push checks...\" && npm run linters",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
